#ad { padding-top: 22px; }

#adside { width: 160px; float: left; margin-left: 32px; margin-top: 20px; }

#footer { max-width: 1000px; height: 115px; background-image: url("images/footer_bg.jpg"); border: 5px solid rgb(242, 242, 242); margin: 10px auto; }

#footer p { text-align: center; text-transform: uppercase; font-size: 0.9em; font-family: Arial, Helvetica, sans-serif; color: rgb(0, 0, 0); font-weight: 700; padding-top: 38px; margin: 0px; }

#footer a { color: rgb(0, 0, 0); font-size: 0.9em; }

.small { font-size: 0.8em; }

.sound { width: 301px; padding-bottom: 12px; border-top: 1px solid rgb(226, 226, 226); }

audio { width: 301px; height: 25px; }

details { display: block; line-height: 200%; cursor: pointer; color: rgb(109, 156, 200); }

@media only screen and (max-width: 1010px) {
  #header { width: 100%; height: 180px; margin: 0px; border: none; }
  #logo { max-height: 135px; padding-top: 62px; }
  #logo h1 { margin: 0px; }
  #page { border-right: none; border-left: none; }
  #footer { width: 100%; margin: 0px; border: none; }
  .section2 { width: 92%; }
  .section3, .section4 { width: 100%; }
  .post, .post2, .post3 { width: 44%; margin-right: 16px; margin-left: 0px; }
  .post-right, .post2-right, .post3-right { width: 44%; margin-right: 0px; margin-left: 0px; }
  img { max-width: 100%; height: auto; }
  post img { width: 100%; height: auto; margin-top: 16px; margin-bottom: 16px; }
  img.left { max-width: 30%; margin: 0px 20px 0px 0px; }
  img.right { max-width: 30%; margin: 0px 0px 0px 20px; }
}

@media only screen and (max-width: 890px) {
  #adside { margin-left: 2px; }
  #search { width: 64%; margin: 0px 15px; }
}

@media screen and (min-width: 771px) {
  .nav-text { display: none; }
}

@media only screen and (max-width: 770px) {
  #header { max-width: 770px; background-image: url("images/piano_bg770x180.jpg"); }
  #footer { max-width: 770px; background-image: url("images/footer_bg770x115.jpg"); }
  #sidebar { width: 100%; height: 70px; }
  #search { width: 42%; clear: none; margin: 3px 0px; }
  input[type="text"] { background-position: 7px 11px; padding: 15px 9px 16px 36px; }
  nav { cursor: pointer; width: 50%; height: 30px; margin-bottom: 37px; float: left; }
  .nav-text { background-image: url("images/nav.png"); background-position: 65% 50%; background-repeat: no-repeat; color: rgb(153, 60, 55); font-size: 1.275em; text-align: center; width: 97%; padding: 5px 0px; margin: 3px 0px 0px 2%; font-weight: 300; border: 1px solid rgb(221, 221, 221); }
  .nav > ul { text-align: center; height: 0px; margin-top: 0px; overflow: hidden; position: relative; z-index: 999; }
  .nav > ul.show-menu { background-color: rgb(255, 255, 255); width: 200%; height: auto; float: left; border: none; box-shadow: rgb(204, 204, 204) 0px 12px 26px -2px; margin: 0px; }
  .nav li { width: 98%; float: right; list-style-type: none; }
  .nav ul li { padding-left: 0px; }
  .nav a { font-size: 0.875em; padding: 8px 0px; display: block; }
  .blue { padding: 12px 0px 8px; margin-left: -12px; }
  .green, .pink { padding: 12px 0px 8px; margin-top: -15px; margin-left: -12px; }
  #content { width: 94%; float: none; padding-right: 3%; padding-left: 3%; }
  .sidebar { display: none; }
  #adside { display: none; }
}

@media only screen and (max-width: 680px) {
  #header { max-width: 680px; height: 175px; background-image: url("images/piano_bg680x175.jpg"); }
  #footer { max-width: 680px; height: 100px; background-image: url("images/footer_bg680x100.jpg"); }
}

@media only screen and (max-width: 620px) {
  #search { width: 40%; }
  .post, .post-right, .post2, .post2-right, .post3, .post3-right { text-align: center; width: 100%; margin-bottom: 8px; margin-right: 0px; padding: 0px; }
  .post2 h2, .post2-right h2, .post h2, .post-right h2, .post3 h2, .post3-right h2 { text-align: center; }
}

@media only screen and (max-width: 500px) {
  #header { max-width: 500px; background-image: url("images/piano_bg500x175.jpg"); }
  .nav-text { background-position: 75% 50%; }
  #search { width: 38%; }
  #footer { max-width: 500px; background-image: url("images/footer_bg500x100.jpg"); }
}

@media only screen and (max-width: 470px) {
  .nav-text { width: 96%; }
  #search { width: 37%; }
}

@media only screen and (max-width: 380px) {
  #header { max-width: 380px; height: 170px; background-image: url("images/piano_bg380x170.jpg"); }
  #footer { max-width: 380px; background-image: url("images/footer_bg380x100.jpg"); }
  .nav-text { width: 95%; }
  #search { width: 35%; }
}

@media only screen and (max-width: 320px) {
  #logo h1 { padding-left: 70px; }
  #logo p { padding-left: 42px; }
  .nav-text { width: 93%; }
  #search { width: 30%; }
}
